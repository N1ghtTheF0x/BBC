{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "H:/SFML/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.19041.0",
            "compilerPath": "H:/mingw64/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerArgs": [
                "-g",
                "src/*.cpp",
                "-o",
                "bin/game.exe",
                "-IH:/SFML/include",
                "-LH:/SFML/lib",
                "-lmingw32",
                "-lsfml-graphics",
                "-lsfml-window",
                "-lsfml-system",
                "-mwindows"
            ]
        }
    ],
    "version": 4
}